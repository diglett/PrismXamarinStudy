<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
                  prism:ViewModelLocator.AutowireViewModel="True"
                  x:Class="PrismXamarinStudy.Views.MasterDetailPageView">
  <MasterDetailPage.Master>
    <ContentPage Title="Default">
      <StackLayout>
        <Button Text="MainPage" Command="{Binding NavigateCommand}" CommandParameter="NavigationPageView/MainPageView" />
        <Button Text="SecondPage" Command="{Binding NavigateCommand}" CommandParameter="NavigationPageView/SecondPageView" />
      </StackLayout>
    </ContentPage>
  </MasterDetailPage.Master>
</MasterDetailPage>